<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>menu.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CreateTable</servlet-name>
        <servlet-class>hagan.b.CreateTable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpsertData</servlet-name>
        <servlet-class>hagan.b.UpsertData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryTable</servlet-name>
        <servlet-class>hagan.b.QueryTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateTable</servlet-name>
        <url-pattern>/CreateTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpsertData</servlet-name>
        <url-pattern>/UpsertData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryTable</servlet-name>
        <url-pattern>/QueryTable</url-pattern>
    </servlet-mapping>
</web-app>